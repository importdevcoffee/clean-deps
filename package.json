{
  "name": "@importdevcoffee/clean-deps",
  "version": "1.1.0",
  "description": "clean-deps is a CLI tool for detecting and removing unused dependencies with optional flags.",
  "license": "MIT",
  "type": "module",
  "main": "bin/clean-deps.js",
  "files": [
    "bin/",
    "src/",
    "package.json",
    "README.md"
  ],
  "bin": {
    "clean-deps": "./bin/clean-deps.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/importdevcoffee"
  },
  "publishConfig": {
    "access": "restricted"
  },
  "private": false,
  "keywords": [
    "dependencies",
    "unused-dependencis",
    "cleanup-tool",
    "cli-tool",
    "cmd",
    "command-line",
    "devtools"
  ],
  "scripts": {
    "scan": "node bin/clean-deps scan",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "prettier": "npx prettier src --check",
    "prettier:fix": "npm run prettier -- --write",
    "test": "vitest",
    "prepare": "npm run lint && npm run prettier && npm run scan"
  },
  "devDependencies": {
    "@eslint/js": "^9.25.1",
    "eslint": "^9.25.1",
    "globals": "^16.0.0",
    "prettier": "3.5.3",
    "vitest": "^3.1.2"
  },
  "dependencies": {
    "acorn": "^8.14.1",
    "chalk": "^5.4.1",
    "commander": "^13.1.0",
    "fast-glob": "^3.3.3"
  }
}
